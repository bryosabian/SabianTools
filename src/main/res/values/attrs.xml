    <declare-styleable name="SabianButtonText">
        <attr name="sbt_text" format="string"></attr>
        <attr name="sbt_hint" format="string"></attr>
        <attr name="sbt_icon" format="string"></attr>
        <attr name="sbt_textColor" format="color"></attr>
