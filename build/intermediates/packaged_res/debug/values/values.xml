        <attr format="enum" name="sbt_icon_alignment">
            <enum name="right" value="0"/>
            <enum name="left" value="1"/>
        </attr>
        <attr format="string" name="sbt_style"/>
        <attr format="dimension" name="sbt_padding"/>
        <attr format="dimension" name="sbt_icon_size"/>
        <attr format="boolean" name="sbt_display_icon"/>
